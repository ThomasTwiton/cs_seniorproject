@model server.Models.AuditionSearch
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<tr><td>Ensemble</td><td>Ensemble ID</td><td>Audition ID</td><td>Instrument Name</td><td>Open Date</td><td>Close Date</td><td>Location</td><td>Description</td></tr><br />

@foreach (var ins in Model.Auditions)
{
    <tr><td>@ins.Ensemble.Ensemble_Name  </td><td>@ins.EnsembleId  </td><td>@ins.AuditionId  </td><td>@ins.Instrument_Name  </td><td>@ins.Open_Date  </td><td>@ins.Closed_Date  </td><td>@ins.Audition_Location  </td><td>@ins.Audition_Description>  </td><td><a asp-action="Audition" asp-route-id="@ins.AuditionId">  PLUG IN  </a></td></tr><br />
}